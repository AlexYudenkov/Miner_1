<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFEB3B"
        android:backgroundTint="@android:color/holo_purple"
        tools:context=".MainActivity">

        <ImageView
            android:id="@+id/groundbot"
            android:layout_width="410dp"
            android:layout_height="273dp"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/ic_groundbot" />

        <ImageView
            android:id="@+id/Tunnel"
            android:layout_width="693dp"
            android:layout_height="1055dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_newtunnel" />

        <ImageView
            android:id="@+id/groundbot2"
            android:layout_width="199dp"
            android:layout_height="824dp"
            android:layout_marginTop="416dp"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_groundbot" />

        <ImageView
            android:id="@+id/groundtop"
            android:layout_width="411dp"
            android:layout_height="781dp"
            android:layout_marginBottom="100dp"
            android:scaleType="centerInside"
            app:layout_constraintBottom_toBottomOf="@+id/groundbot"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/ic_groundtop" />

        <ImageView
            android:id="@+id/bur"
            android:layout_width="245dp"
            android:layout_height="317dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_newboer" />

        <ImageView
            android:id="@+id/menu"
            android:layout_width="37dp"
            android:layout_height="31dp"
            android:contentDescription="TODO"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.954"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.024"
            app:srcCompat="@drawable/ic_menu" />

        <ImageView
            android:id="@+id/exit"
            android:layout_width="40dp"
            android:layout_height="33dp"
            android:contentDescription="TODO"
            app:layout_constraintBottom_toBottomOf="@+id/menu"
            app:layout_constraintEnd_toStartOf="@+id/menu"
            app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/menu"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/ic_exit" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="273dp"
            android:layout_height="110dp"
            android:layout_marginTop="72dp"
            app:layout_constraintEnd_toEndOf="@+id/menu"
            app:layout_constraintHorizontal_bias="0.984"
            app:layout_constraintStart_toEndOf="@+id/exit"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_info" />

        <TextView
            android:id="@+id/resources"
            android:layout_width="83dp"
            android:layout_height="20dp"
            android:layout_marginTop="16dp"
            android:alpha="0.5"
            android:text="resources"
            android:textColor="#BEBEBE"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView3"
            app:layout_constraintHorizontal_bias="0.163"
            app:layout_constraintStart_toStartOf="@+id/imageView3"
            app:layout_constraintTop_toTopOf="@+id/imageView3" />

        <TextView
            android:id="@+id/resources2"
            android:layout_width="83dp"
            android:layout_height="20dp"
            android:layout_marginTop="16dp"
            android:alpha="0.5"
            android:text="0"
            android:textColor="#BEBEBE"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView3"
            app:layout_constraintHorizontal_bias="0.763"
            app:layout_constraintStart_toEndOf="@+id/resources"
            app:layout_constraintTop_toTopOf="@+id/imageView3" />

        <TextView
            android:id="@+id/fuel2"
            android:layout_width="83dp"
            android:layout_height="20dp"
            android:layout_marginTop="12dp"
            android:alpha="0.5"
            android:text="0"
            android:textColor="#BEBEBE"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView3"
            app:layout_constraintHorizontal_bias="0.763"
            app:layout_constraintStart_toEndOf="@+id/fuel"
            app:layout_constraintTop_toBottomOf="@+id/resources2" />

        <TextView
            android:id="@+id/record2"
            android:layout_width="83dp"
            android:layout_height="20dp"
            android:layout_marginTop="12dp"
            android:alpha="0.5"
            android:text="0"
            android:textColor="#BEBEBE"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView3"
            app:layout_constraintHorizontal_bias="0.763"
            app:layout_constraintStart_toEndOf="@+id/recorddepth"
            app:layout_constraintTop_toBottomOf="@+id/fuel2" />

        <TextView
            android:id="@+id/recorddepth"
            android:layout_width="83dp"
            android:layout_height="20dp"
            android:layout_marginTop="12dp"
            android:alpha="0.5"
            android:text="record depth"
            android:textColor="#BEBEBE"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView3"
            app:layout_constraintHorizontal_bias="0.163"
            app:layout_constraintStart_toStartOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/fuel" />

        <TextView
            android:id="@+id/fuel"
            android:layout_width="83dp"
            android:layout_height="20dp"
            android:layout_marginTop="12dp"
            android:alpha="0.5"
            android:text="fuel"
            android:textColor="#BEBEBE"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView3"
            app:layout_constraintHorizontal_bias="0.163"
            app:layout_constraintStart_toStartOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/resources" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>
